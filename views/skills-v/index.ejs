<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <title>My Dev Skills List</title>
</head>
<body>
    <h1>DEV SKILLS</h1>
    <h3><a href="/skills/new">Add a new Skill</a></h3>
    <ul>
        <% list.forEach(function(sk, idx){ %>
            <li>
                <a href="/skills/<%= idx %>">
                    <%= sk.skill %>
                </a>
                - Level: <div><%= sk.level %></div>

            </li>
        <% }) %>
    </ul>
</body>
</html>